        if (result.errors.length > 0) {\n          console.warn('Sync errors:', result.errors);\n        }\n      } catch (error) {\n        console.error('Full sync error:', error);\n        Alert.default.alert(t('error'), 'Failed to perform two-way sync');\n      } finally {\n        setIsSyncing(false);\n      }\n    };\n\n    // Sync app contacts TO device (App → iPhone)\n    const syncToDevice = async () => {\n      if (!syncService || isSyncing) return;\n      setIsSyncing(true);\n      try {\n        const result = await syncService.syncAppToDevice();\n        await fetchContacts();\n        const total = result.synced + result.created;\n        const message = total > 0 ? `✓ Updated ${result.synced}, created ${result.created} contacts on iPhone` : 'All contacts are already synced to iPhone';\n        setLastSyncResult(message);\n        Alert.default.alert('App → iPhone Complete', message);\n        if (result.errors.length > 0) {\n          console.warn('Sync errors:', result.errors);\n        }\n      } catch (error) {\n        console.error('Sync error:', error);\n        Alert.default.alert(t('error'), 'Failed to sync to iPhone');\n      } finally {\n        setIsSyncing(false);\n      }\n    };\n\n    // Sync from iPhone TO app (iPhone → App)\n    const syncFromDevice = async () => {\n      if (!syncService || isSyncing) return;\n      setIsSyncing(true);\n      try {\n        const result = await syncService.syncDeviceToApp();\n        await fetchContacts();\n        const messages = [];\n        if (result.synced > 0) messages.push(`${result.synced} updated`);\n        if (result.imported > 0) messages.push(`${result.imported} imported`);\n        const message = messages.length > 0 ? `✓ ${messages.join(', ')} from iPhone` : 'All contacts are already in sync';\n        setLastSyncResult(message);\n        Alert.default.alert('iPhone → App Complete', message);\n        if (result.errors.length > 0) {\n          console.warn('Sync errors:', result.errors);\n        }\n      } catch (error) {\n        console.error('Sync error:', error);\n        Alert.default.alert(t('error'), 'Failed to sync from iPhone');\n      } finally {\n        setIsSyncing(false);\n      }\n    };\n\n    // Link existing app contacts with device contacts\n    const linkContacts = async () => {\n      if (!syncService || isSyncing) return;\n      setIsSyncing(true);\n      try {\n        const result = await syncService.linkExistingContacts();\n        await fetchContacts();\n        const message = result.linked > 0 ? `✓ Linked ${result.linked} contacts with iPhone` : 'All contacts are already linked';\n        setLastSyncResult(message);\n        Alert.default.alert('Linking Complete', message);\n      } catch (error) {\n        console.error('Link error:', error);\n        Alert.default.alert(t('error'), 'Failed to link contacts');\n      } finally {\n        setIsSyncing(false);\n      }\n    };\n    const fetchContacts = async () => {\n      if (!token) return;\n      try {\n        const response = await axios.default.get(`${EXPO_PUBLIC_BACKEND_URL}/api/contacts`, getAuthHeaders());\n        setContacts(response.data);\n      } catch (error) {\n        console.error('Error fetching contacts:', error);\n      } finally {\n        setLoading(false);\n      }\n    };\n    const fetchDrafts = async () => {\n      if (!token) return;\n      try {\n        const response = await axios.default.get(`${EXPO_PUBLIC_BACKEND_URL}/api/drafts`, getAuthHeaders());\n        setDrafts(response.data);\n      } catch (error) {\n        console.error('Error fetching drafts:', error);\n      }\n    };\n    const fetchGroups = async () => {\n      if (!token) return;\n      try {\n        const response = await axios.default.get(`${EXPO_PUBLIC_BACKEND_URL}/api/groups`, getAuthHeaders());\n        setGroups(response.data || []);\n      } catch (error) {\n        console.error('Error fetching groups:', error);\n      }\n    };\n\n    // Fetch profile to get dynamic pipeline stages\n    const fetchProfile = async () => {\n      if (!token) return;\n      try {\n        const response = await axios.default.get(`${EXPO_PUBLIC_BACKEND_URL}/api/profile`, getAuthHeaders());\n        const profile = response.data;\n        if (profile.pipeline_stages && profile.pipeline_stages.length > 0) {\n          // Sort by interval and add \"New\" at the beginning\n          const sorted = [...profile.pipeline_stages].filter(s => s.enabled !== false).sort((a, b) => a.interval_days - b.interval_days);\n          const stageNames = ['New', ...sorted.map(s => s.name)];\n          setPipelineStages(stageNames);\n          setPipelineStagesConfig(sorted);\n        }\n      } catch (error) {\n        // Use default stages if profile not found\n        console.log('Using default pipeline stages');\n      }\n    };\n\n    // Copy text to clipboard\n    const copyToClipboard = async text => {\n      try {\n        await Clipboard.setStringAsync(text);\n        triggerHaptic('success');\n        Alert.default.alert('✓', 'Copied to clipboard');\n      } catch (error) {\n        console.error('Error copying to clipboard:', error);\n        Alert.default.alert('Error', 'Failed to copy');\n      }\n    };\n    (0, _react.useEffect)(() => {\n      if (token) {\n        fetchContacts();\n        fetchDrafts();\n        fetchGroups();\n        fetchProfile();\n        // Don't auto-sync on startup - let user tap the sync button\n        // This avoids race conditions with the contacts API\n      }\n    }, [token]);\n\n    // Auto-refresh when screen is focused (e.g., after importing contacts or editing)\n    (0, _expoRouter.useFocusEffect)((0, _react.useCallback)(() => {\n      if (token) {\n        fetchContacts();\n        fetchDrafts();\n        fetchGroups();\n        fetchProfile(); // Also refresh pipeline stages\n      }\n    }, [token]));\n\n    // Also refresh when app comes to foreground\n    (0, _react.useEffect)(() => {\n      const subscrip