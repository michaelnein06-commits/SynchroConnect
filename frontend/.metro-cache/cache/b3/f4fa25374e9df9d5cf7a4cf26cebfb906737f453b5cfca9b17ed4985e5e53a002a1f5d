ContactsModal(false);\n        setSelectedGroupForContacts(null);\n        setSelectedContactsForGroup([]);\n        fetchContacts();\n        fetchGroups();\n        Alert.default.alert('✓', `Added ${selectedContactsForGroup.length} contacts to ${selectedGroupForContacts.name}`);\n      } catch (error) {\n        console.error('Error adding contacts to group:', error);\n        Alert.default.alert('Error', 'Failed to add contacts to group');\n      }\n    };\n    const getDaysUntilDue = nextDue => {\n      if (!nextDue) return null;\n      const due = new Date(nextDue);\n      const now = new Date();\n      const diff = Math.ceil((due.getTime() - now.getTime()) / (1000 * 60 * 60 * 24));\n      return diff;\n    };\n    const generateDraft = async (contactId, contactName) => {\n      try {\n        await axios.default.post(`${EXPO_PUBLIC_BACKEND_URL}/api/drafts/generate/${contactId}`, {}, getAuthHeaders());\n        Alert.default.alert('Success', `AI draft generated for ${contactName}!`);\n        fetchDrafts();\n        setActiveTab('drafts');\n      } catch (error) {\n        Alert.default.alert('Error', 'Failed to generate draft');\n      }\n    };\n    const filteredContacts = contacts.filter(c => c.name.toLowerCase().includes(searchQuery.toLowerCase()) || c.job && c.job.toLowerCase().includes(searchQuery.toLowerCase()));\n    const renderContactCard = (contact, showDraftButton = true, isDraggable = false) => {\n      const daysUntil = getDaysUntilDue(contact.next_due);\n      const isOverdue = daysUntil !== null && daysUntil < 0;\n      const isNewContact = contact.pipeline_stage === 'New';\n      const stageColor = getStageColor(contact.pipeline_stage);\n      return /*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(TouchableOpacity.default, {\n        style: [styles.contactCard, isOverdue && !isNewContact && styles.contactCardOverdue],\n        onPress: () => router.push(`/contact/${contact.id}`),\n        onLongPress: () => handleLongPressContact(contact),\n     