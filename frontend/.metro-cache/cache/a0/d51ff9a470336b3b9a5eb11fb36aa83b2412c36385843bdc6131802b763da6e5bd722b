tSyncResult(message);\n        Alert.default.alert('iPhone → App Complete', message);\n        if (result.errors.length > 0) {\n          console.warn('Sync errors:', result.errors);\n        }\n      } catch (error) {\n        console.error('Sync error:', error);\n        Alert.default.alert(t('error'), 'Failed to sync from iPhone');\n      } finally {\n        setIsSyncing(false);\n      }\n    };\n\n    // Link existing app contacts with device contacts\n    const linkContacts = async () => {\n      if (!syncService || isSyncing) return;\n      setIsSyncing(true);\n      try {\n        const result = await syncService.linkExistingContacts();\n        await fetchContacts();\n        const message = result.linked > 0 ? `✓ Linked ${result.linked} contacts with iPhone` : 'All contacts are already linked';\n        setLastSyncResult(message);\n        Alert.default.alert('Linking Complete', message);\n      } catch (error) {\n        console.error('Link error:', error);\n        Alert.default.alert(t('error'), 'Failed to link contacts');\n      } finally {\n        setIsSyncing(false);\n      }\n    };\n    const fetchContacts = async () => {\n      if (!token) return;\n      try {\n        const response = await axios.default.get(`${EXPO_PUBLIC_BACKEND_URL}/api/contacts`, getAuthHeaders());\n        setContacts(response.data);\n      } catch (error) {\n        console.error('Error fetching contacts:', error);\n      } finally {\n        setLoading(false);\n        setRefreshing(false);\n      }\n    };\n    const fetchDrafts = async () => {\n      if (!token) return;\n      try {\n        const response = await axios.default.get(`${EXPO_PUBLIC_BACKEND_URL}/api/drafts`, getAuthHeaders());\n        setDrafts(response.data);\n      } catch (error) {\n        console.error('Error fetching drafts:', error);\n      }\n    };\n    const fetchGroups = async () => {\n      if (!token) return;\n      try {\n        const response = await axios.default.get(`${EXPO_PUBLIC_BACKEN